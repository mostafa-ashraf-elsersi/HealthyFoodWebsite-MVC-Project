
<!-- Page Header Start -->
<div class="container-fluid page-header wow fadeIn" data-wow-delay="0.1s">
    <div class="container d-none d-md-block">
        <h1 class="display-3 mb-3 animated slideInDown">Password Change Notification</h1>
        <nav aria-label="breadcrumb animated slideInDown">
            <ol class="breadcrumb mb-0">
                <li class="breadcrumb-item"><a class="text-body" asp-controller="Home" asp-action="GetView">Home</a></li>
                <li class="breadcrumb-item text-dark active" aria-current="page">Password Change Notification</li>
            </ol>
        </nav>
    </div>
</div>
<!-- Page Header End -->
<!-- Notification Card Start -->
<div class="container-fluid bg-light bg-icon py-6">
    <div class="container">
        <div class="text-center mx-auto mb-5 wow fadeInUp" data-wow-delay="0.1s" style="max-width: 500px;">
            <div class="text-start">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title mt-3">Notification!</h5>
                        <p style="text-align:justify;">An email with a password changing link is supposed to be sent to you! Check your email inbox, change your old password, then sign in with the new one!</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Notification Card End -->
